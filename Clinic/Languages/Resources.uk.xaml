<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <sys:String x:Key="UsernameLabel" xmlns:sys="clr-namespace:System;assembly=mscorlib">Логін</sys:String>
    <sys:String x:Key="PasswordLabel" xmlns:sys="clr-namespace:System;assembly=mscorlib">Пароль</sys:String>
    <sys:String x:Key="LoginButton" xmlns:sys="clr-namespace:System;assembly=mscorlib">Увійти</sys:String>
    <sys:String x:Key="LangLabel" xmlns:sys="clr-namespace:System;assembly=mscorlib">Мова</sys:String>
